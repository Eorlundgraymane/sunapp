        $(function() {
            var CACHE_COOKIE = 'rufc';
            var __cv =  $.cookie(CACHE_COOKIE);
            console.log(__cv);
            if (__cv == "true") {
                $(".content").html("Page Expired");
                // window.location.reload();
                console.log('reloaded');
            }
            $.cookie(CACHE_COOKIE, 'true', { path: '/' });
        });
