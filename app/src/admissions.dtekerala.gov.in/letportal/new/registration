        <script type="text/javascript" >
            $('.carousel').carousel({
                interval: 5000 //changes the speed
            });
            var __webPath__ = '/letportal/bundles/?v16';
            $("[title]").tooltip();
            $(".nav a").on("click", function () {
                $(".nav").find(".active").removeClass("active");
                $(this).parent().addClass("active");
            });
        </script>
                    
    <script type="text/javascript" src="/letportal/bundles/app/admin/news_ticker/scripts/jquery.bootstrap.newsbox.min.js?v16"></script>
    <script type="text/javascript">
        $(function () {

            $(".news-ticker").bootstrapNews({
                newsPerPage: 4,
                autoplay: true,
                pauseOnHover: false,
                //navigation: false,
                direction: 'up',
                newsTickerInterval: 2500,
                onToDo: function () {
                }
            });
        });

        $(".panel-body").on('click', '.view-more', function (e) {
            e.preventDefault();
            var content = $(this).data('data');
            $("#view-more-box").html(content);
        });

        $('#view-all').click(function () {
            var $path = "/letportal/viewAll";
            $.ajax({
                url: $path,
                type: 'POST',
                data: {'view': 'user'},
                beforeSend: function () {
                    $("#view-all-box").html('');
                    $("#view-all-modal .loading").append('<div class="overlay"></div><div class="loading-img"></div>');
                },
                success: function ($data) {
                    $("#view-all-box").html($data);
                },
                complete: function () {
                    $("#view-all-modal .loading").find(".overlay, .loading-img").remove();
                }
            });
        });

        $('.registrationblock').click(function () {
            alert('Online Registration Stopped');
        });
    </script>


