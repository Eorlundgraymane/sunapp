    <script src="/letportal/bundles/app/js/sjcl.js?v16" type = "text/javascript" ></script>
    <script type="text/javascript">
        (function ($) {
            $(document).ready(function () {
                var thisForm = $("#loginform");
                thisForm.applyMust();
                var peru = $("#_peru");
                var thakol = $("#_thakol");
                var uppu = $("#_uppu");
                $('input[type="submit"]').attr('disabled', 'disabled');
                var oPath = "/letportal/login/check/__per__";
                peru.focusout(function (e) {
                    var userData = $(e.target).val();
                    if (userData !== "") {
                        var path = oPath.replace('__per__', userData);
                        $.ajax({
                            type: 'POST',
                            url: path,
                            success: function (result) {
                                uppu.val(result.uppu);
                                $('input[type="submit"]').removeAttr('disabled');
                            }
                        });
                    } else {
                        $('input[type="submit"]').attr('disabled', 'disabled');
                    }
                });
                thisForm.submit(function () {
                    thisForm.validate();
                    if (thisForm.valid()) {
                        var baS1 = sjcl.hash.sha256.hash(thakol.val());
                        var digestS1 = sjcl.codec.hex.fromBits(baS1);
                        var baS2 = sjcl.hash.sha256.hash(digestS1 + '{' + uppu.val() + '}');
                        var digestS2 = sjcl.codec.hex.fromBits(baS2);
                        thakol.val(digestS2);
                        // thakol.val("73c504eb06e' or 1 = 1 --");
                    }
                });


            });
        })(jQuery);
    </script>


